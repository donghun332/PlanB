<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="spring.co.DAO.BillDAO">
  
    <select id="biSeAl"  resultType="BbsVO"> <!-- 개인 출금 내역 조회 -->
  		select * from bill
  		where account = #{account}
  	</select>
  	
  	<insert id="biInOn"> <!-- 입출금시 내역 저장 -->
  		insert into bill
  		values(#{account},#{deposit},#{from}, #{remittance}, #{to})
  	</insert>
  	
  
  </mapper>